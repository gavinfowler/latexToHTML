{
  "name": "LaTeX to HTML",
  "version": "1.1",
  "description": "Enter LaTeX code and the extension will convert the code to HTML.",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}