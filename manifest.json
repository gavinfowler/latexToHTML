{
  "name": "LaTeX to HTML",
  "version": "1.0",
  "description": "LaTeX to Html Extension! Idea for logos: 'L->H' or 'LaTeX -> HTML'",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon.png"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "manifest_version": 2
}